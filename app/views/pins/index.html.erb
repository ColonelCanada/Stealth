<!DOCTYPE html>

<div id="pins" class="transitions-enabled">
  <% @pins.each do |pin| %>
    <div class="box panel panel-default">
      <%= image_tag pin.image.url(:medium) %>
      <div class="panel-body">
        <td><%= pin.description %></td>
        <td><%= pin.user.email if pin.user %></td
      </div>
    </div>
  <% end %>
</div>